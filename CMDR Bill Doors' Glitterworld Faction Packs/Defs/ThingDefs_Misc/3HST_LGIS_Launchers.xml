<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--==================== Single LGIS Launcher ===============  -->
  <ThingDef ParentName="BaseMakeableGun">
    <defName>3HST_LGIS_Launcher</defName>
    <label>3HST LGIS missile Launcher</label>
    <description>Small caliber infantry-portable missile launcher employed by 3HST grenadiers. Built-in low-power rail accelerator gives missile greater range.</description>
    <graphicData>
      <texPath>Things/Weapons/3HST_IWL_09_Single_Launcher</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Interact_Rifle</soundInteract>
    <statBases>
      <WorkToMake>62500</WorkToMake>
      <Mass>2</Mass>
      <Bulk>6</Bulk>
      <SwayFactor>0</SwayFactor>
      <ShotSpread>-114514</ShotSpread>
      <SightsEfficiency>3</SightsEfficiency>
      <RangedWeapon_Cooldown>0.85</RangedWeapon_Cooldown>
    </statBases>
    <costList>
      <Steel>30</Steel>
      <Plasteel>20</Plasteel>
      <Chemfuel>10</Chemfuel>
      <ComponentIndustrial>10</ComponentIndustrial>
      <ComponentSpacer>1</ComponentSpacer>
    </costList>
    <verbs>
      <li Class="CombatExtended.VerbPropertiesCE">
        <verbClass>CombatExtended.Verb_ShootCE</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>Projectile_70mmLGIS_Frag</defaultProjectile>
        <ai_IsBuildingDestroyer>true</ai_IsBuildingDestroyer>
        <warmupTime>4.4</warmupTime>
        <burstShotCount />
        <range>200</range>
        <minRange>10</minRange>
        <targetParams>
          <canTargetLocations>true</canTargetLocations>
        </targetParams>
        <soundCast>InfernoCannon_Fire</soundCast>
        <soundCastTail>GunTail_Medium</soundCastTail>
        <muzzleFlashScale>5</muzzleFlashScale>
        <ignorePartialLoSBlocker>true</ignorePartialLoSBlocker>
      </li>
    </verbs>
    <!--
    <modExtensions>
      <li Class="CombatExtended.GunDrawExtension">
        <DrawSize>0.93,0.93</DrawSize>
        <DrawOffset>0.0,0.0</DrawOffset>
      </li>
    </modExtensions>
    -->
    <comps>
      <li Class="CombatExtended.CompProperties_AmmoUser">
        <magazineSize>1</magazineSize>
        <reloadTime>8.6</reloadTime>
        <ammoSet>AmmoSet_70mmLGIS</ammoSet>
      </li>
      <li Class="CombatExtended.CompProperties_FireModes">
        <aiAimMode>AimedShot</aiAimMode>
      </li>
    </comps>
    <weaponTags>
      <li>GunHeavy</li>
    </weaponTags>
    <tools>
      <li Class="CombatExtended.ToolCE">
        <label>stock</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8</power>
        <cooldownTime>1.55</cooldownTime>
        <chanceFactor>1.5</chanceFactor>
        <armorPenetrationBlunt>2.755</armorPenetrationBlunt>
        <linkedBodyPartsGroup>Stock</linkedBodyPartsGroup>
      </li>
      <li Class="CombatExtended.ToolCE">
        <label>barrel</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>5</power>
        <cooldownTime>2.02</cooldownTime>
        <armorPenetrationBlunt>1.630</armorPenetrationBlunt>
        <linkedBodyPartsGroup>Barrel</linkedBodyPartsGroup>
      </li>
      <li Class="CombatExtended.ToolCE">
        <label>muzzle</label>
        <capacities>
          <li>Poke</li>
        </capacities>
        <power>8</power>
        <cooldownTime>1.55</cooldownTime>
        <armorPenetrationBlunt>2.755</armorPenetrationBlunt>
        <linkedBodyPartsGroup>Muzzle</linkedBodyPartsGroup>
      </li>
    </tools>
    <modExtensions>
      <li Class="CombatExtended.GunDrawExtension">
        <DrawSize>1.3,1.3</DrawSize>
        <DrawOffset>-0.3,0.0</DrawOffset>
      </li>
    </modExtensions>

    <recipeMaker>
      <researchPrerequisite>CE_Launchers</researchPrerequisite>
      <recipeUsers Inherit="False">
        <li>FabricationBench</li>
      </recipeUsers>
    </recipeMaker>
  </ThingDef>
  
