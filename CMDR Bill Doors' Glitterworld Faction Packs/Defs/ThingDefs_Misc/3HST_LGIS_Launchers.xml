  <RecipeDef ParentName="GrenadeRecipeBase">
    <defName>MakeDLGISfrag</defName>
    <label>make disposable frag LGIS launcher x5</label>
    <description>Craft 5 disposable frag LGIS launchers.</description>
    <jobString>Making frag LGIS launchers.</jobString>
    <workAmount>1500</workAmount>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>15</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>FSX</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Ammo_70mmLGIS_Frag</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
        <li>FSX</li>
        <li>ComponentIndustrial</li>
        <li>Ammo_70mmLGIS_Frag</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Gun_3HST_LGIS_Launcher_Disposable_frag>5</Gun_3HST_LGIS_Launcher_Disposable_frag>
    </products>
    <researchPrerequisite>CE_Launchers</researchPrerequisite>
  </RecipeDef>

