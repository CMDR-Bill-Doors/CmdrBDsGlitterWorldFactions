  <!--====================== Multiple high-angle LGIS Turret ===============-->
  <ThingDef ParentName="BaseArtilleryBuilding">
    <defName>Turret_MLGIS_ID</defName>
    <label>Multiple high-angle LGIS turret</label>
    <constructionSkillPrerequisite>15</constructionSkillPrerequisite>
    <graphicData>
      <drawSize>(2,2)</drawSize>
      <texPath>Things/Building/Security/TurretHeavy_Base</texPath>
    </graphicData>
    <statBases>
      <MaxHitPoints>500</MaxHitPoints>
      <WorkToBuild>150000</WorkToBuild>
      <Mass>1000</Mass>
      <Bulk>1000</Bulk>
    </statBases>
    <techLevel>Spacer</techLevel>
    <comps>
      <li Class="CompProperties_Flickable"/>
      <li Class="CompProperties_Breakdownable"/>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>1200</basePowerConsumption>
      </li>
    </comps>
    <fillPercent>0.9</fillPercent>
    <description>A multiple LGIS missile turret with it's targeting system set to indirect fire.</description>
    <costList>
      <Steel>1150</Steel>
      <Plasteel>385</Plasteel>
      <Chemfuel>285</Chemfuel>
      <ComponentIndustrial>13</ComponentIndustrial>
      <ComponentSpacer>5</ComponentSpacer>
    </costList>
    <specialDisplayRadius>15</specialDisplayRadius>
    <building>
      <turretGunDef>Gun_MLGIS_ID</turretGunDef>
      <ai_combatDangerous>true</ai_combatDangerous>
      <turretTopDrawSize>4</turretTopDrawSize>
      <buildingTags>
        <li>Artillery_BaseDestroyer</li>
        <li>Artillery_MannedMortar</li>
      </buildingTags>
    </building>
    <placeWorkers>
      <li>PlaceWorker_NotUnderRoof</li>
      <li>PlaceWorker_TurretTop</li>
    </placeWorkers>
    <size>(2,2)</size>
    <minifiedDef>MinifiedThing</minifiedDef>
    <researchPrerequisites>
      <li>CE_TurretHeavyWeapons</li>
    </researchPrerequisites>
    <designationCategory>Security</designationCategory>
  </ThingDef>

  <ThingDef ParentName="BaseArtilleryWeapon">
    <defName>Gun_MLGIS_ID</defName>
    <label>multiple LGIS missile launcher</label>
    <description>A multiple rocket launcher capable of holding and firing 27 LGIS missiles.  Built-in low-power rail accelerator gives missile greater range.</description>
    <graphicData>
      <texPath>Things/Building/Turrets/3HST_XML_25_MLGISL</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Artillery_ShellLoaded</soundInteract>
    <statBases>
      <SightsEfficiency>5</SightsEfficiency>
      <ShotSpread>4</ShotSpread>
      <SwayFactor>0</SwayFactor>
      <RangedWeapon_Cooldown>0.35</RangedWeapon_Cooldown>
      <MarketValue>16000</MarketValue>
    </statBases>
    <verbs>
      <li Class="CombatExtended.VerbPropertiesCE">
        <verbClass>CombatExtended.Verb_ShootMortarCE</verbClass>
