  <RecipeDef ParentName="AmmoRecipeBase">
    <defName>MakeAmmo_70mmLGIS_HEAT</defName>
    <label>make LGIS HEAT missiles x5</label>
    <description>Craft 5 LGIS HEAT missiles.</description>
    <jobString>Making LGIS HEAT missiles.</jobString>
    <workAmount>13500</workAmount>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>15</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>FSX</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
        <li>FSX</li>
        <li>Plasteel</li>
        <li>ComponentIndustrial</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Ammo_70mmLGIS_HEAT>5</Ammo_70mmLGIS_HEAT>
    </products>
  </RecipeDef>

